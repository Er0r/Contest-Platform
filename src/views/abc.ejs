			};
			firebase.initializeApp(firebaseConfig);
			firebase.analytics();
		</script>

		<!-- Firebase Config Done -->
		<script>
			var capacity = " <%= capacity %>";
			sessionStorage.setItem('capacity', capacity);
			
		</script>
		<script src="//cdn.ckeditor.com/4.15.1/standard/ckeditor.js"></script>
		<script src="/socket.io/socket.io.js"></script>
		<script type="module" src="Assets/js/rtc.js"></script>
		<script
			src="https://cdnjs.cloudflare.com/ajax/libs/webrtc-adapter/7.3.0/adapter.min.js"
			integrity="sha256-2qQheewaqnZlXJ3RJRghVUwD/3fD9HNqxh4C+zvgmF4="
			crossorigin="anonymous"
		></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>

		<title>Title</title>

	</head>
	<body>
		<div id="loader" class="center"></div>
		<div class="card shadow-lg bg-transparent">
			<nav class="navbar bg-transparent rounded-0 d-print-none">
				<div>
					<div style="height: 5rem; width: 15rem">
						<a href="/">
							<img
								src="../Assets/css/1.png"
								alt=""
								style="height: 100%; width: 100%; object-fit: contain"
							/>
						</a>
					</div>
				</div>

				<div id="teamdiv" style="display: none">
					<label class="text-white" for="">
						<span id="teamid" class="font-weight-bold text-uppercase"></span
					></label>
				</div>
				<div id="waitingdiv" hidden>
					<label class="text-white" for="">
						<span id="waitingid" class="font-weight-bold text-uppercase">Please Wait For Other Members</span
					></label>
				</div>
				<div class="bg-transparent text-center">
					<div class="card-body blink_text" style="color: white">
						<h1><span id="time"></span></h1>
					</div>
				</div>
					<div id="setteamdiv" style="display: hidden">
						<div
						
						class="d-flex align-items-center justify-content-center"
						
					>
						<input
							type="text"
							placeholder="Enter Team Name"
							class="form-control"
							id="teamname"
						/>
						<button
							class="btn btn-success m-1"
							id="confirmbtn"
							onClick="window.location.reload();"
							hidden
							
						>
							Confirm
						</button>

						<button
							class="btn btn-danger m-1"
							id="leave"
							hidden
						>
							Leave
						</button>
					</div>
				</div>
				
			</nav>
		</div>
		<div class="container-fluid room-comm" hidden>
			<div class="row">
				<video class="local-video" id="local" volume="0" autoplay muted></video>
			</div>
			<div
				class="row"
				style="display: flex; flex-direction: row; padding-top: 3rem"
			>
				<div class="col-md-5 mt-3 mb-3 main">
					<div class="row;" id="videos"></div>
				</div>
			</div>

			<div
				class="card shadow-md p-2 col-6 chat-col bg-transparent float-right"
				id="chat-pane"
			>
				<div class="">
					<textarea name="content"></textarea>
				</div>
				<script>
					CKEDITOR.replace("content");
				</script>
				<button class="btn btn-warning m-3" id="postbtn" hidden>POST</button>

				<div class="shadow card">
					<div class="card-body" style="color: black" id="card"></div>
				</div>

				<script src="../Assets/js/posthandler.js"></script>
				<!-- Handler js-->
				<div>
					<% if(capacity === 4) { %>
					<script>
						initializeTimer(1000);
					</script>
					<% } else if(capacity === 3 || capacity === 2) { %>
					<script>
						initializeTimer(3000); // arektu baraite hobe // 
					</script>
					<% } %>
					<script>
						showTime();
					</script>
				</div>
			</div>
		</div>
		<script src="../Assets/js/loader.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.slim.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.1/umd/popper.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.6.0/js/bootstrap.min.js"></script>
	</body>
</html>
